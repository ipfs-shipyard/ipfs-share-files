{
  "name": "ipfs-share-files",
  "version": "0.0.0",
  "description": "Share files via IPFS",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "cp config/webpack.config.prod.js node_modules/react-scripts/config/",
    "build": "react-scripts build",
    "build:icons": "svgr -d src/icons node_modules/ipfs-css/icons && standard --fix src/icons/*",
    "lint": "standard",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:serve": "ecstatic build --cache=0 --port=3001",
    "test:e2e": "react-scripts test --testMatch **/test/e2e/**/*.test.js",
    "test:e2e:ci": "URL=http://localhost:3001 run-p --race test:serve test:e2e",
    "analyze": "source-map-explorer build/static/js/main.*",
    "bundlesize": "bundlesize",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@svgr/cli": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-extension": "^4.0.5",
    "filesize": "^3.6.1",
    "internal-nav-helper": "^1.0.2",
    "ipfs-css": "^0.6.0",
    "ipfs-redux-bundle": "^1.0.0",
    "milliseconds": "^1.0.3",
    "prop-types": "^15.6.2",
    "pull-file-reader": "^1.0.2",
    "react": "^16.4.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.4.1",
    "redux-bundler": "^21.2.2",
    "redux-bundler-react": "^1.0.1",
    "tachyons": "^4.10.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "video-extensions": "^1.1.0",
    "window-or-global": "^1.0.1",
    "window.ipfs-fallback": "^1.1.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^3.4.8",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-runtime": "^6.26.0",
    "big.js": "^5.1.2",
    "bundlesize": "^0.17.0",
    "ecstatic": "^3.2.1",
    "eslint": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "multihashing-async": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "puppeteer": "^1.5.0",
    "source-map-explorer": "^1.5.0",
    "standard": "^11.0.1"
  },
  "homepage": "./",
  "standard": {
    "parser": "babel-eslint"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "512 kB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "18 kB"
    }
  ]
}
