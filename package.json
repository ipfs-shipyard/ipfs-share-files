{
  "name": "ipfs-share-files",
  "version": "0.0.0",
  "description": "Share files via IPFS",
  "leadMaintainer": "Diogo Silva <fsdiogo@gmail.com>",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "cp config/webpack.config.prod.js node_modules/react-scripts/config/",
    "build": "react-scripts build",
    "build:icons": "svgr -d src/icons node_modules/ipfs-css/icons && eslint --fix src/icons/*",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:serve": "ecstatic build --cache=0 --port=3001",
    "test:e2e": "react-scripts test --testMatch **/test/e2e/**/*.test.js",
    "test:e2e:ci": "URL=http://localhost:3001 run-p --race test:serve test:e2e",
    "analyze": "source-map-explorer build/static/js/main.*",
    "bundlesize": "bundlesize",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@svgr/cli": "^5.5.0",
    "classnames": "^2.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "file-extension": "^4.0.5",
    "filesize": "^6.1.0",
    "i18next": "^19.8.5",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-icu": "^1.4.2",
    "i18next-xhr-backend": "^3.2.2",
    "internal-nav-helper": "^3.1.0",
    "ipfs-css": "^1.3.0",
    "ipfs-http-client": "^49.0.0",
    "ipfs-provider": "^1.1.0",
    "is-ipfs": "^2.0.0",
    "milliseconds": "^1.0.3",
    "multiaddr": "^8.1.2",
    "multiaddr-to-uri": "^6.0.0",
    "prop-types": "^15.7.2",
    "pull-file-reader": "^1.0.2",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.1",
    "react-circular-progressbar": "^2.0.3",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.5",
    "react-intl": "^5.11.0",
    "react-loader-spinner": "^4.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-test-renderer": "^17.0.1",
    "redux-bundler": "^28.0.0",
    "redux-bundler-react": "^1.2.0",
    "shortid": "^2.2.16",
    "tachyons": "^4.12.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "video-extensions": "^1.1.0",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.15",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-knobs": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/addons": "^6.1.15",
    "@storybook/react": "^6.1.15",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-runtime": "^6.26.0",
    "big.js": "^6.0.3",
    "bundlesize": "^0.18.1",
    "ecstatic": "^4.1.4",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ipfs": "^0.54.0",
    "ipfsd-ctl": "^7.2.0",
    "multihashing-async": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^5.5.0",
    "source-map-explorer": "^2.5.2"
  },
  "homepage": "./",
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "512 kB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "18 kB"
    }
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
